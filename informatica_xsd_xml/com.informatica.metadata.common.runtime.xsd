<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="targetCommitInterval" type="xsd:int"/>
<xsd:attribute name="maximumDataErrors." type="imx:idref"/>
<xsd:attribute name="maximumRowsReadPerPartition." type="imx:idref"/>
<xsd:attribute name="maximumRuntimeIntervalPerSource." type="imx:idref"/>
<xsd:attribute name="selectedExecutionEnvironmentName." type="imx:idref"/>
<xsd:attribute name="stateStore." type="imx:idref"/>
