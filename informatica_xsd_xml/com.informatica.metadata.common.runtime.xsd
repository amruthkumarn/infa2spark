<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="TxHandlerConfig" type="txhandlerconfig:TxHandlerConfig"/>
</xsd:choice>
</xsd:complexType>
</xsd:element>
<xsd:element name="streamingConfig" type="runtime:StreamingConfig"/>
