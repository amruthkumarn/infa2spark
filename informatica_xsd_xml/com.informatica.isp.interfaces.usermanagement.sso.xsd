<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="portal" type="xsd:boolean"/>
<xsd:attribute name="resetHostPort" type="xsd:boolean"/>
<xsd:attribute name="userName">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="version">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="NodeLoginParameter" type="sso:NodeLoginParameter"/>
<xsd:complexType name="CredentialData">
<xsd:complexContent>
<xsd:extension base="common:IdObj">
<xsd:attribute name="active" type="xsd:boolean"/>
<xsd:attribute name="nonExpiring" type="xsd:boolean"/>
<xsd:attribute name="renewTimeInterval" type="xsd:int"/>
<xsd:attribute name="service" type="xsd:boolean"/>
<xsd:attribute name="userType">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="CredentialData" type="sso:CredentialData"/>
<xsd:complexType name="UserSession">
<xsd:complexContent>
<xsd:extension base="common:IdObj">
<xsd:attribute name="admin" type="xsd:boolean"/>
<xsd:attribute name="credential">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="expirationTime" type="xsd:long"/>
<xsd:attribute name="parentGroups" type="imx:idrefs"/>
<xsd:attribute name="service" type="xsd:boolean"/>
<xsd:attribute name="unionedPrivileges" type="imx:idrefs"/>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="UserSession" type="sso:UserSession"/>
</xsd:schema>
