<xsd:complexType name="SerializedModelExtension">
<xsd:complexContent>
<xsd:extension base="sample:BaseModelExtension">
<xsd:annotation>
<xsd:documentation> Serialized sample model extension


</xsd:documentation>
</xsd:annotation>
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="genericContainer" type="genericextension:ExtensionContainer"/>
</xsd:choice>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="SerializedModelExtension" type="sample:SerializedModelExtension"/>
</xsd:schema>
