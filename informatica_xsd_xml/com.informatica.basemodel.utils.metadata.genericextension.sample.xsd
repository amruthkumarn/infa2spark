</xsd:complexType>
</xsd:element>
</xsd:choice>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="ExtendedAggregateSampleAdapter" type="sample:ExtendedAggregateSampleAdapter"/>
