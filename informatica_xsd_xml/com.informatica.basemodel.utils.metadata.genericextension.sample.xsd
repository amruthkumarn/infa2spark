<xsd:attribute name="longProperty" type="xsd:long"/>
<xsd:attribute name="shortProperty" type="xsd:short"/>
<xsd:attribute name="stringProperty">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="ExtendedAggregatedModelExtension" type="sample:ExtendedAggregatedModelExtension"/>
<xsd:complexType name="ExtendedModelExtension">
<xsd:complexContent>
<xsd:extension base="sample:BaseModelExtension">
<xsd:annotation>
<xsd:documentation> sample of model extensions


</xsd:documentation>
</xsd:annotation>
<xsd:attribute name="blobProperty">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="booleanProperty" type="xsd:boolean"/>
<xsd:attribute name="byteProperty" type="xsd:byte"/>
<xsd:attribute name="charProperty">
<xsd:simpleType>
<xsd:restriction base="imx:char">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="dateProperty">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="doubleProperty" type="xsd:double"/>
<xsd:attribute name="enumProperty" type="sample:ExtensionEnumValue"/>
<xsd:attribute name="floatProperty" type="xsd:float"/>
