<xsd:extension base="common1:ISPObject">
<xsd:annotation>
<xsd:documentation> Model Change Log
 
 2.9.0 brikumar 06/3/2015 Added new field serviceType.

 This model will be used for sending Response for Application Service Request.
 @author brikumar

</xsd:documentation>
</xsd:annotation>
<xsd:attribute name="isSuccess" type="xsd:boolean"/>
<xsd:attribute name="requestId">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="requestType" type="common:RequestType"/>
<xsd:attribute name="serviceName">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
<xsd:attribute name="serviceType">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="ApplicationServiceRequestResponseInfo" type="common:ApplicationServiceRequestResponseInfo"/>
<xsd:complexType name="ISPEncryptedString">
<xsd:complexContent>
<xsd:extension base="common1:ISPObject">
<xsd:attribute name="string">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="ISPEncryptedString" type="common:ISPEncryptedString"/>
<xsd:complexType name="NodeRequestInfo">
<xsd:complexContent>
<xsd:extension base="common1:ISPObject">
