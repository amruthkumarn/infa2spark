<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="HPort" type="hierarchy:HPort"/>
</xsd:choice>
</xsd:complexType>
</xsd:element>
</xsd:choice>
<xsd:attribute ref="imx:id"/>
<xsd:attribute ref="imx:idref"/>
<xsd:attribute ref="imx:iid"/>
<xsd:attribute name="port" type="imx:idref"/>
</xsd:complexType>
