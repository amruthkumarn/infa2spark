</xsd:complexType>
<xsd:element name="RHComponent" type="hierarchy:RHComponent"/>
<xsd:complexType name="RHGroup">
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element ref="imx:annotations"/>
<xsd:element name="injectionPoints">
<xsd:complexType>
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="InjectionPoint" type="hierarchy:InjectionPoint"/>
</xsd:choice>
</xsd:complexType>
</xsd:element>
<xsd:element name="ports">
<xsd:complexType>
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="HPort" type="hierarchy:HPort"/>
</xsd:choice>
</xsd:complexType>
</xsd:element>
</xsd:choice>
<xsd:attribute ref="imx:id"/>
<xsd:attribute ref="imx:idref"/>
<xsd:attribute ref="imx:iid"/>
<xsd:attribute name="port" type="imx:idref"/>
</xsd:complexType>
<xsd:element name="RHGroup" type="hierarchy:RHGroup"/>
<xsd:simpleType name="XMLValidationType">
<xsd:restriction base="xsd:string">
<xsd:enumeration value="noValidation"/>
<xsd:enumeration value="warnOnInvalidXML"/>
<xsd:enumeration value="errorOnInvalidXML"/>
</xsd:restriction>
</xsd:simpleType>
<xsd:complexType name="XPath">
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element ref="imx:annotations"/>
</xsd:choice>
<xsd:attribute ref="imx:id"/>
<xsd:attribute ref="imx:idref"/>
<xsd:attribute ref="imx:iid"/>
<xsd:attribute name="path">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:whiteSpace value="preserve"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
<xsd:element name="XPath" type="hierarchy:XPath"/>
</xsd:schema>
