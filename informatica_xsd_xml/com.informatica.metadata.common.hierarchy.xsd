<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element ref="imx:annotations"/>
<xsd:element name="groups">
<xsd:complexType>
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="RHGroup" type="hierarchy:RHGroup"/>
</xsd:choice>
</xsd:complexType>
</xsd:element>
<xsd:element name="namespaces">
<xsd:complexType>
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="NamespaceToPrefixMap" type="hierarchy:NamespaceToPrefixMap"/>
</xsd:choice>
</xsd:complexType>
</xsd:element>
</xsd:choice>
