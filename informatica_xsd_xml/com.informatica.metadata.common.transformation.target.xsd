</xsd:restriction>
</xsd:simpleType>
<xsd:complexType name="TargetDataInterface">
<xsd:complexContent>
<xsd:extension base="resourceaccess:ResourceAccessDataInterface">
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="targetFields">
<xsd:complexType>
<xsd:choice maxOccurs="unbounded" minOccurs="0">
<xsd:element name="TargetTxField" type="target:TargetTxField"/>
</xsd:choice>
</xsd:complexType>
</xsd:element>
</xsd:choice>
</xsd:extension>
</xsd:complexContent>
</xsd:complexType>
<xsd:element name="TargetDataInterface" type="target:TargetDataInterface"/>
